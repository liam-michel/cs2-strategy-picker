{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "bun --env-file .env.dev --watch src/server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "db:dev": "docker-compose -f docker-compose.yml --env-file .env.dev up -d",
    "db:test": "docker-compose -f docker-compose.yml --env-file .env.test up -d",
    "db:migratedev": "bun --env-file .env.dev prisma migrate dev",
    "db:migratetest": "bun --env-file .env.test prisma migrate dev",
    "db:studiodev": "bun --env-file .env.dev prisma studio",
    "db:studiotest": "bun --env-file .env.test prisma studio",
    "db:generatedev": "bun --env-file .env.dev prisma generate",
    "db:generatetest": "bun --env-file .env.test prisma generate",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "bun@2.2.0",
  "dependencies": {
    "@cs2monorepo/shared": "workspace:*",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@trpc/server": "^11.9.0",
    "better-auth": "^1.4.18",
    "dotenv": "^17.3.1",
    "fastify": "^5.7.2",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "react-hook-form": "^7.71.1",
    "superjson": "^2.2.6",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@prisma/client": "6.6.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.1.0",
    "jest": "^30.2.0",
    "prisma": "6.6.0",
    "testdouble": "^3.20.2",
    "testdouble-jest": "^2.0.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
