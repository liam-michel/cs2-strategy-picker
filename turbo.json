{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    },
    "db:dev": {
      "cache": false
    },
    "db:test": {
      "cache": false
    },
    "db:migratedev": {
      "cache": false,
      "persistent": false
    },
    "db:migratetest": {
      "cache": false,
      "persistent": false
    },
    "db:studiodev": {
      "cache": false,
      "persistent": true
    },
    "db:studiotest": {
      "cache": false,
      "persistent": true
    },
    "db:generatedev": {
      "cache": false
    },
    "db:generatetest": {
      "cache": false
    }
  }
}
